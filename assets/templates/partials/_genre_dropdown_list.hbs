{{#each genres }}
    <span class="dropdown-item-text">
        {{ this.title }}
        {{#if this.isApproved }}
            <i class="fas fa-check"></i>
        {{ else }}
            <i class="far fa-thumbs-down"></i>
        {{/if}}
    </span>
{{ else }}
    <span class="dropdown-item-text">
        <i>no genres</i>
    </span>
{{/each}}
