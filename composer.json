{
    "name": "onlytracker/onlytracker",
    "description": "Parse. Analyze. Persistence. Search",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "backup-manager/symfony": "^2.2",
        "beberlei/assert": "^3.2",
        "ocramius/generated-hydrator": "^3.0",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.1",
        "snc/redis-bundle": "^3.2",
        "symfony/console": "5.0.*",
        "symfony/dom-crawler": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.4",
        "symfony/form": "5.0.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/http-client": "5.0.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/twig-bundle": "5.0.*",
        "symfony/validator": "5.0.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.0.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "nelmio/alice": "^3.5",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/debug-pack": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "theofidry/psysh-bundle": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "OnlyTracker\\BackEnd\\": "applications/onlytracker_backend/src",
            "OnlyTracker\\": ["src"]
        },
        "files": [

        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OnlyTracker\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
