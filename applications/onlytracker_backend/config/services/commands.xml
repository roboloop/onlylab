<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="false" autowire="false" autoconfigure="true" />

        <service id="OnlyTracker\BackEnd\Command\App\LoadTopicCommand">
            <argument type="service" id="OnlyTracker\Infrastructure\Request\OnlyTracker\OnlyTrackerRequestSender"/>
            <argument type="service" id="OnlyTracker\Application\Handler\TopicPageHandlerInterface"/>
        </service>

        <service id="OnlyTracker\BackEnd\Command\App\LoadForumCommand">
            <argument type="service" id="OnlyTracker\Infrastructure\Request\OnlyTracker\OnlyTrackerRequestSender"/>
            <argument type="service" id="OnlyTracker\Application\Handler\ForumPageHandlerInterface"/>
        </service>

    </services>
</container>
